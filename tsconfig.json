{
  "extends": "@quasar/app-vite/tsconfig-preset",
  "baseUrl": "./",
  "allowJs": true,
  "compilerOptions": {
    "lib": ["es5", "es6", "dom", "dom.iterable"],
    "paths": {
      "@components/*": ["./src/components/*"],
      "@assets/*": ["./src/assets/*"],
      "@constant/*": ["./src/constant/*"],
      "@stores/*": ["./src/stores/*"],
      "@interfaces/*": ["./src/interfaces/*"]
    }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "lodash.d.ts"
  ]
}
